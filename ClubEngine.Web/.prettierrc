    "printWidth": 200,
    "semi": true,
    "bracketSameLine": true,
    "singleAttributePerLine": true,
    "trailingComma": "es5",
    "tabWidth": 2,
    "singleQuote": true,
    "bracketSpacing": true,
    "arrowParens": "always",
    "plugins": ["prettier-plugin-organize-imports", "prettier-plugin-tailwindcss"]
}
